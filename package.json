{
    "name": "rezervo-web",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "prod": "run-s build start",
        "prettier": "prettier --write '**/*.{js,ts,tsx,md,json,yaml,css}' --ignore-path=.gitignore",
        "prettier:check": "prettier --check '**/*.{js,ts,tsx,md,json,yaml,css}' --ignore-path=.gitignore",
        "lint": "next lint --ignore-path=.gitignore",
        "lint:fix": "next lint --fix --ignore-path=.gitignore",
        "typecheck": "tsc --noEmit",
        "check": "run-p prettier:check lint typecheck",
        "fix": "run-s prettier lint:fix",
        "review": "run-s check prod"
    },
    "dependencies": {
        "@auth0/nextjs-auth0": "^3.1.0",
        "@emotion/react": "^11.11.1",
        "@emotion/styled": "^11.11.0",
        "@mui/icons-material": "^5.14.3",
        "@mui/lab": "^5.0.0-alpha.140",
        "@mui/material": "^5.14.5",
        "js-cookie": "^3.0.5",
        "luxon": "^3.4.0",
        "next": "^13.4.19",
        "next-pwa": "^5.6.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "sharp": "^0.32.5",
        "swr": "^2.2.1"
    },
    "devDependencies": {
        "@types/js-cookie": "^3.0.4",
        "@types/luxon": "^3.3.2",
        "@types/node": "^20.8.0",
        "@types/react": "^18.2.23",
        "@types/react-dom": "^18.2.8",
        "@typescript-eslint/eslint-plugin": "^6.7.3",
        "@typescript-eslint/parser": "^6.7.3",
        "eslint": "^8.50.0",
        "eslint-config-next": "^13.5.3",
        "eslint-config-prettier": "^9.0.0",
        "eslint-import-resolver-typescript": "^3.6.1",
        "eslint-plugin-deprecation": "^2.0.0",
        "eslint-plugin-import": "^2.28.1",
        "eslint-plugin-no-relative-import-paths": "^1.5.2",
        "npm-run-all": "^4.1.5",
        "prettier": "^3.0.3",
        "typescript": "^5.2.2"
    }
