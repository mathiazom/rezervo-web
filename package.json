        "@mui/x-date-pickers": "^7.28.3",
        "@serwist/next": "^9.0.13",
        "js-cookie": "^3.0.5",
        "jwt-decode": "^4.0.0",
        "luxon": "^3.6.1",
        "next": "15.3.0",
        "react": "^19.1.0",
        "react-device-detect": "^2.2.3",
        "react-dom": "^19.1.0",
