        "@mui/x-date-pickers": "^7.24.1",
        "@serwist/next": "^9.0.11",
        "js-cookie": "^3.0.5",
        "jwt-decode": "^4.0.0",
        "luxon": "^3.5.0",
        "next": "15.1.3",
        "nuqs": "^2.3.1",
        "react": "^19.0.0",
        "react-device-detect": "^2.2.3",
        "react-dom": "^19.0.0",
        "react-dropzone": "^14.3.5",
