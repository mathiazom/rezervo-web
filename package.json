        "@eslint/js": "^9.24.0",
        "@next/eslint-plugin-next": "^15.3.0",
        "@tsconfig/next": "^2.0.3",
        "@tsconfig/strictest": "^2.0.5",
        "@types/js-cookie": "^3.0.6",
